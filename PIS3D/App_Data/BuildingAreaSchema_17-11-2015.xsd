<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
attributeFormDefault="unqualified">


  <xs:annotation>
    <xs:documentation xml:lang="en">
      Building Area Schema
      Copyright 2013, GETS Zamil Steel. All rights reserved
    </xs:documentation>
  </xs:annotation>

  <xs:element name="BuildingArea">
    <xs:complexType>
      <xs:all>
        <xs:element name="GeneralData" type="BuildingAreaParametersType"/>
        <xs:element name="DesignCode" type="DesignCodesType"/>
        <xs:element name="SurfaceTreatment" type="SurfaceTreatmentType"/>
        <xs:element name="Bracings">
          <xs:complexType>
            <xs:all>
              <xs:element name="NSW" type="BracingType"/>
              <xs:element name="NSWHeight" type="xs:double" default="0"/>
              <xs:element name="FSW" type="BracingType"/>
              <xs:element name="FSWHeight" type="xs:double" default="0"/>
              <xs:element name="LEW" type="BracingType"/>
              <xs:element name="LEWHeight" type="xs:double" default="0"/>
              <xs:element name="REW" type="BracingType"/>
              <xs:element name="REWHeight" type="xs:double" default="0"/>
              <xs:element name="InteriorBracings">
                <xs:complexType>
                  <xs:all minOccurs="0">
                    <xs:element name="Bracing" type="BracingTypeLocation"/>
                  </xs:all>
                </xs:complexType>
              </xs:element>
              <xs:element name="Remarks" type="RemarksType"> </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="PanelsAndInsulations">
          <xs:complexType>
            <xs:all>
              <xs:element name="SingleSkinTypes">
                <xs:complexType>
                  <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="SingleSkin" type="CladdSkin"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="TempconTypes">
                <xs:complexType>
                  <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="Tempcon" type="CladdTempcon"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="InsulationTypes">
                <xs:complexType>
                  <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="Insulation" type="Insulation"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="RoofWallCladding" type="RoofWallCladdingType"/>
        <xs:element name="WallPanelAccessories" type="WallPanelAccessoriesType"/>
        <xs:element name="Openings" type="OpeningsType"/>
        <xs:element name="Subsystems" type="StructuralSubsystemsType"/>
        <xs:element name="CraneSystems" type="CraneSystemsType"/>
        <xs:element name="Mezzanines" type="MezzaninesType"/>
      <xs:element name="OldArea" type="xs:string"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="OpeningsType">
    <xs:sequence>

      <xs:element name="Louvers" type="LouversType"/>
      <xs:element name="Windows" type="WindowsType"/>
      <xs:element name="FiberglassRoofCurbs" type="FiberglassRoofCurbsType"/>
      <xs:element name="GravityRidgeVentillators" type="GravityRidgeVentillatorsType"/>
      <xs:element name="OtherSBO" type="OtherSBOType"/>
      <xs:element name="PersonnelDoors" type="PersonnelDoorsType"/>
      <xs:element name="PipeFlashings" type="PipeFlashingsType"/>
      <xs:element name="PowerVentilators" type="PowerVentilatorsType"/>
      <xs:element name="RollupDoors" type="RollupDoorsType"/>
      <xs:element name="SlidingDoors" type="SlidingDoorsType"/>
      <xs:element name="FramedOpenings" type="FramedOpeningsType"/>
      <xs:element name="OpenWalls" type="OpenWallsType"/>
      <xs:element name="TranslucentPanels" type="TranslucentPanelsType"/>


    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="ByEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ZS"/>
      <xs:enumeration value="OTHER"/>
      <xs:enumeration value="NONE"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="RemarkType">
    <xs:all>
      <xs:element name="ID" type="xs:integer" default="0" />
      <xs:element name="Remark" type="xs:string"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="RemarksType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="Remark" type="RemarkType"/>
    </xs:sequence>
  </xs:complexType>


  <xs:simpleType name="SurfaceEnum">
    <xs:restriction base="xs:string">

      <xs:enumeration value="NSW"/>
      <xs:enumeration value="FSW"/>
      <xs:enumeration value="LEW"/>
      <xs:enumeration value="REW"/>
      <xs:enumeration value="ROOF"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="EaveConditionType">
    <xs:all>
      <xs:element name="EaveConditionEnum" type="EaveConditionEnumType" default="NONE"/>
      <xs:element name="Downspouts" minOccurs="0" default="NONE">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="STANDARD"/>
            <xs:enumeration value="PVC"/>
            <xs:enumeration value="FG OUTLET ONLY"/>
            <xs:enumeration value="BY OTHERS"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="EaveConditionEnumType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NO EAVE"/>
      <xs:enumeration value="EAVE GUTTER"/>
      <xs:enumeration value="EAVE TRIM"/>
      <xs:enumeration value="CURVED W/O PROJ"/>
      <xs:enumeration value="CURVED W/O PROJ WITH V.G"/>
      <xs:enumeration value="CURVED WITH PROJ"/>
      <xs:enumeration value="CURVED WITH PROJ WITH V.G"/>
      <xs:enumeration value="VALLEY GUTTER"/>
      <xs:enumeration value="NONE"/>

    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="BuildingAreaParametersType">
    <xs:all>
      <xs:element name="ID" type="xs:integer" default="01"/>
      <xs:element name="Location">
        <xs:complexType>
          <xs:all>
            <xs:element name="AlongBuildingWidth" type="WidthType"/>
            <xs:element name="AlongBuildingLength" type="WidthType"/>
          </xs:all>
        </xs:complexType>
      </xs:element>

      <xs:element name="Width" type="xs:double" default="20"/>
      <xs:element name="WidthReference" default="OUT TO OUT OF STEEL LINE (O/O OF EAVE STRUTS)">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="OUT TO OUT OF STEEL LINE (O/O OF EAVE STRUTS)"/>
            <xs:enumeration value="OUT TO OUT OF COLUMN FLANGES"/>
            <xs:enumeration value="CENTERLINE TO CENTERLINE OF COLUMN BASES"/>
            <xs:enumeration value="INSIDE TO INSIDE OF COLUMN INNER FLANGES AT KNEES"/>
            <xs:enumeration value="OUT TO OUT OF BLOCK WALLS"/>
            <xs:enumeration value="OTHER (See Remarks)"/>

          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Length" type="xs:double" default="8"/>
      <xs:element name="LengthReference" default="OUT TO OUT OF STEEL LINE (O/O ENDWALL GIRTS)">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="OUT TO OUT OF STEEL LINE (O/O ENDWALL GIRTS)"/>
            <xs:enumeration value="OUT TO OUT OF COLUMN FLANGES"/>
            <xs:enumeration value="CENTERLINE TO CENTERLINE OF COLUMN BASES"/>
            <xs:enumeration value="INSIDE TO INSIDE OF ENDWALL COLUMN INNER FLANGES AT KNEES"/>
            <xs:enumeration value="OTHER (See Remarks)"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="HeightReference" default="EAVE HEIGHT">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="EAVE HEIGHT"/>
            <xs:enumeration value="CLEAR HEIGHT"/>
            <xs:enumeration value="RIDGE HEIGHT"/>
            <xs:enumeration value="OTHER (See Remarks)"/>

          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="FrameType">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="CLEAR SPAN"/>
            <xs:enumeration value="MULTI SPAN"/>
            <xs:enumeration value="LEAN TO"/>
            <xs:enumeration value="MULTI GABLE"/>
            <xs:enumeration value="ROOF SYSTEM"/>
            <xs:enumeration value="FLAT ROOF"/>
            <xs:enumeration value="SINGLE SLOPE"/>
            <xs:enumeration value="CAR CANOPY: FALCON"/>
            <xs:enumeration value="CAR CANOPY: BUTTERFLY"/>
            <xs:enumeration value="CAR CANOPY: CARACAL"/>
            <xs:enumeration value="CAR CANOPY: CHEETAH"/>
            <xs:enumeration value="CAR CANOPY: GAZELLE"/>
            <xs:enumeration value="OTHER (See Remarks)"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FixedBaseAllowed" type="xs:boolean" default="true"/>
      <xs:element name="Gables">
        <xs:complexType>
          <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element name="Gable" type="GableType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="Bays">
        <xs:complexType>
          <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element name="Bay" type="BayType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="EndWalls">
        <xs:complexType>
          <xs:all>
            <xs:element name="LEW" type="EndWallType"/>
            <xs:element name="REW" type="EndWallType"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="Profiles">
        <xs:complexType>
          <xs:sequence minOccurs="1" maxOccurs="unbounded">
            <xs:element name="Profile" type="ProfileType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Frames">
        <xs:complexType>
          <xs:sequence minOccurs="2" maxOccurs="unbounded">
            <xs:element name="FrameProfile" type="FrameProfileType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="BaysProfile" type="xs:string" minOccurs="0"/>
      <xs:element name="FramesProfile" type="xs:string" minOccurs="0"/>
      <xs:element name="FutureExtension">
        <xs:complexType>
          <xs:all>
            <xs:element name="TransverseDirection" type="xs:boolean"/>
            <xs:element name="LongitudinalDirection" type="xs:boolean"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="Remarks" type="RemarksType"> </xs:element>

    </xs:all>
  </xs:complexType>
  <xs:complexType name="ProfileType">
    <xs:all>
      <xs:element name="ID" type="xs:integer" default="0" />

      <xs:element name="GablesWidthModules">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded" minOccurs="1">

            <xs:element name="GableWidthModules">
              <xs:complexType>
                <xs:sequence maxOccurs="unbounded">
                  <xs:element name="GableNo" type="xs:integer"  default="1"> </xs:element>
                  <xs:element name="WidthModules">
                    <xs:complexType>
                      <xs:sequence minOccurs="1" maxOccurs="unbounded">
                        <xs:element name="Width" type="xs:double" default="20.000"> </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>

          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="EndWallType">
    <xs:all>

      <xs:element name="Type" default="POST AND BEAM">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="POST AND BEAM"/>
            <xs:enumeration value="RIGID FRAME"/>
            <xs:enumeration value="RIGID FRAME WITH HALF BAY LOADING"/>
            <xs:enumeration value="COMMON FRAME IN OTHER AREA"/>
            <xs:enumeration value="FALSE RAFTER"/>
            <xs:enumeration value="PART. RIGID FRAME PART. P AND B"/>
            <xs:enumeration value="OTHER"/>
            <xs:enumeration value="NONE"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Profile" type="ProfileType"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="GableType">

    <xs:all>
      <xs:element name="ID" type="xs:integer" default="1" />

      <xs:element name="LeftSlope" type="xs:double" default="0.1"/>
      <xs:element name="RightSlope" type="xs:double" default="-0.1"/>
      <xs:element name="Width" type="xs:double" default="20"/>
      <xs:element name="RidgeDistance" type="xs:double" default="10"/>
      <xs:element name="LeftHeight" type="xs:double" default="6"/>
      <xs:element name="LeftColBase" type="xs:double" default="0.0"/>
      <xs:element name="RightHeight" type="xs:double" default="6"/>
      <xs:element name="RightColBase" type="xs:double" default="0.0"/>
      <xs:element name="EaveCondition">
        <xs:complexType>
          <xs:all>
            <xs:element name="NearSide" type="EaveConditionType"/>
            <xs:element name="FarSide" type="EaveConditionType"/>
          </xs:all>
        </xs:complexType>
      </xs:element>

    </xs:all>
  </xs:complexType>
  <xs:complexType name="BayType">

    <xs:all>
      <xs:element name="ID" type="xs:integer"
      default="01"/>
      <xs:element name="Width" type="xs:double" default="8"/>
      <xs:element name="BracingAvailable" type="xs:boolean" default="true"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="FrameProfileType">
    <xs:all>
      <xs:element name="ID" type="xs:integer"  default="01"/>
      <xs:element name="ProfileNo" type="xs:integer" default="0"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="WidthType">
    <xs:all>
      <xs:element name="FromGL" type="xs:string"/>
      <xs:element name="ToGL" type="xs:string"/>
      <xs:element name="Axes" type="xs:string"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="DesignCodesType">
    <xs:all>
      <xs:element name="DeadLoad">
        <xs:complexType>
          <xs:all>
            <xs:element name="AsPerDesign" type="xs:string"/>
            <xs:element name="UserValue" type="xs:double" default="0.7"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="LiveLoadOnRoof" type="xs:double" default="1.00"> </xs:element>
      <xs:element name="LiveLoadOnFrame" type="xs:double" default="0.57"/>
      <xs:element name="ThicknessGroundSnow" type="xs:double" default="0"/>
      <xs:element name="GroundSnowLoad" type="xs:double" default="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Roof Snow Load = ( Ground Snow Load x Reduction Factor )
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollateralLoad" type="xs:double" default="0"/>
      <xs:element name="RainFall">
        <xs:complexType>
          <xs:all>
            <xs:element name="Intensity" type="xs:double" default="150"/>
            <xs:element name="HeavyRainArea" type="xs:boolean"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="Temperature">
        <xs:complexType>
          <xs:all>
            <xs:element name="Plus" type="xs:double" default="0"/>
            <xs:element name="Minus" type="xs:double" default="0"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="Wind">
        <xs:complexType>
          <xs:all>
            <xs:element name="WindPressure" type="xs:double" default="1.0" minOccurs="0"/>
            <xs:element name="WindSpeedEnum" minOccurs="0" default="3 SECOND GUST SPEED">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="FASTEST MILE"/>
                  <xs:enumeration value="3 SECOND GUST SPEED"/>
                  <xs:enumeration value="10 MINUTES"/>
                  <xs:enumeration value="NONE"/>
                  <xs:enumeration value="OTHER"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="WindSpeed" type="xs:double" default="144" minOccurs="0"/>
            <xs:element name="WindExposure"  default="B" >
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="B"/>
                  <xs:enumeration value="C"/>
                  <xs:enumeration value="D"/>
                  <xs:enumeration value="NONE"/>
                  <xs:enumeration value="OTHER"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="Seismic">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="OTHER"/>
            <xs:enumeration value="Zone 1"/>
            <xs:enumeration value="Zone 2"/>
            <xs:enumeration value="Zone 3"/>
            <xs:enumeration value="Zone 4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DesignCodes">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="AISC"/>
            <xs:enumeration value="AISC 1989"/>
            <xs:enumeration value="BS-5950-1990"/>
            <xs:enumeration value="BS-5950-2000"/>
            <xs:enumeration value="ECP97"/>
            <xs:enumeration value="ECP01"/>
            <xs:enumeration value="EURO CODE 3"/>
            <xs:enumeration value="AISC05-ASD"/>
            <xs:enumeration value="AISC05-LRFD"/>
            <xs:enumeration value="AISC10-ASD"/>
            <xs:enumeration value="AISC10-LRFD"/>
            <xs:enumeration value="IS800-1984"/>
            <xs:enumeration value="IS800-2007"/>
            <xs:enumeration value="OTHER (See Remarks)"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>


      <xs:element name="ApplyLoadsPer">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="MBMA 2002"/>
            <xs:enumeration value="MBMA 2006"/>
            <xs:enumeration value="IBC 2003"/>
            <xs:enumeration value="IBC 2006"/>
            <xs:enumeration value="UBC 91"/>
            <xs:enumeration value="UBC 97"/>
            <xs:enumeration value="ASCE-7 2002"/>
            <xs:enumeration value="ASCE-7 2005"/>
            <xs:enumeration value="SAUDI ROYAL COMMISSION: YANBU"/>
            <xs:enumeration value="SAUDI ROYAL COMMISSION: JUBAIL"/>
            <xs:enumeration value="BS 6399-CP3"/>
            <xs:enumeration value="ECP 93"/>
            <xs:enumeration value="IS875"/>
            <xs:enumeration value="OTHER (See Remarks)"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ColdFormedDesignCodes">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="AISI 01"/>
            <xs:enumeration value="AISI 80"/>
            <xs:enumeration value="AISI 86"/>
            <xs:enumeration value="IS 801"/>
            <xs:enumeration value="OTHER (See Remarks)"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Remarks" type="RemarksType"> </xs:element>

    </xs:all>
  </xs:complexType>
  <xs:simpleType name="BracingType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="DIAGONAL"/>
      <xs:enumeration value="PORTAL"/>
      <xs:enumeration value="PORTAL PARTIAL HEIGHT"/>
      <xs:enumeration value="MINOR AXIS"/>
      <xs:enumeration value="NONE"/>
      <xs:enumeration value="OTHER"/>

    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="BracingTypeLocation">
    <xs:all>
      <xs:element name="Location">
        <xs:complexType>
          <xs:choice>
            <xs:element name="Offset">
              <xs:complexType>
                <xs:all>
                  <xs:element name="Offset" type="xs:double" default="0"/>
                  <xs:element name="From">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:enumeration value="NSW"/>
                        <xs:enumeration value="LEW"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:all>
              </xs:complexType>
            </xs:element>
            <xs:element name="AtGL" type="xs:string"/>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="Type" type="BracingType"/>
      <xs:element name="PortalPartialHeight" minOccurs="0" type="xs:double" default="0"/>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="SurfaceTreatmentType">
    <xs:all>
      <xs:element name="PrimaryMembers" type="PrimarySurfaceTreatment"/>
      <xs:element name="SecondaryMembers" type="SecondarySurfaceTreatment">
        <xs:annotation xml:lang="en">
          <xs:documentation>
            Secondary members are Purlins, Eave Struts, Girts, Gable Angle and Base Angle
            Other secondary members such as : Endwall Posts, Post And Beam members, Rod And Angle bracing, Portal Frames will be painted with the same paint system as primary members
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="PrimarySurfaceTreatment">
    <xs:all>
      <xs:element name="SurfaceFinish">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="PAINTED"/>
            <xs:enumeration value="HOT DIP GALVANIZED"/>

          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="SurfaceTreatment">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="SHOT BLAST 2.5"/>
            <xs:enumeration value="STANDARD"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="PaintSystem">

        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="NONE"/>
            <xs:enumeration value="RED OXIDE PRIMER ONLY"/>
            <xs:enumeration value="ALUM OXIDE PRIMER ONLY"/>
            <xs:enumeration value="GREY OXIDE PRIMER ONLY"/>
            <xs:enumeration value="SPECIAL"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Coats">
        <xs:complexType>
          <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element type="CoatDataType" name="Coat"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Remarks" type="RemarksType"> </xs:element>

    </xs:all>
  </xs:complexType>
  <xs:complexType name="SecondarySurfaceTreatment">
    <xs:all>
      <xs:element name="SurfaceFinish">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="PAINTED"/>

            <xs:enumeration value="PRE GALVANIZED"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="SurfaceTreatment">
        <xs:simpleType>
          <xs:restriction base="xs:string">

            <xs:enumeration value="NONE"/>
            <xs:enumeration value="STANDARD"/>
            <xs:enumeration value="SWEEP BLAST"/>

          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PaintSystem">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="RED OXIDE PRIMER ONLY"/>
            <xs:enumeration value="ALUM OXIDE PRIMER ONLY"/>
            <xs:enumeration value="GREY OXIDE PRIMER ONLY"/>
            <xs:enumeration value="SPECIAL"/>
