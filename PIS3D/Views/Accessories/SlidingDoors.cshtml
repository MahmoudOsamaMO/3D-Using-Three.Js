   
@{
    ViewBag.Title = " Sliding Doors";
}
<style>
    .Marg {
        margin-top: 10px;
    }
    .modalmportopen{
        overflow-y:auto !important;
    }
</style>

<form ng-controller="SlidingDoorsCtrl" ng-submit="submit()"
      name="myForm" novalidate>
    <hr />
    <div>
        <div id="foo" ng-show="Loading" style="margin-top:20%;">
        </div>
        <div ng-hide="Loading" id="panel" class="panel panel-primary">
            <div class="panel-heading">
                <h4>Sliding Doors </h4>

            </div>
            <div class="panel-body">
                <div class="panel-group" id="accordion">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a class="text-primary" data-toggle="collapse" data-parent="#accordion" href="#collapse5">Summary</a>
                            </h4>
                        </div>
                        <div id="collapse5" class="panel-collapse collapse in">
                            <div class="panel-body">
                                <div class="tab-pane active" id="5b">

                                    <div class="container-fluid">
                                        <div class="row">
                                            @*load*@
                                            <div class="col-lg-12">
                                                <div class="col-lg-12 well table-responsive" style="font-size:12px;">
                                                    <h4 class="text-primary"><u>Sliding Doors</u></h4>
                                                    <table st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                                                        <thead class="text-center text-info">
                                                            <tr>
                                                                <th rowspan="2">NO</th>
                                                                <th colspan="2">Size</th>
                                                                <th colspan="2">Surface</th>
                                                                <th colspan="3">Location</th>
                                                                <th rowspan="2">Type</th>
                                                                <th rowspan="2">Support</th>
                                                                <th rowspan="2">Trim</th>
                                                                <th rowspan="2">Pilot</th>
                                                                <th rowspan="2">Remarks</th>
                                                            </tr>
                                                            <tr>
                                                                <th>Width (mm)</th>
                                                                <th>Height (mm)</th>
                                                                <th>Assign To</th>
                                                                <th>On Bay</th>
                                                                <th>X</th>
                                                                <th>Y</th>
                                                                <th>Of</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="x in building.Openings.SlidingDoors.SlidingDoor track by $index">
                                                                <td>
                                                                    <div>{{x.ID = $index + 1}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Width}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Height}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.AssignTo}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.On}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.LocationX}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.LocationY}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.LocationFrom}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Type}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Support}}</div>
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Trims}}</div>
                                                                </td>
                                                                <td>
                                                                    <input type="checkbox" ng-disabled="true" ng-model="x.PilotDoor" />
                                                                </td>
                                                                <td>
                                                                    <div>{{x.Remarks.Remark[0].Remark}}</div>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>

                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h4 class="panel-title">
                                <a class="text-primary" data-toggle="collapse" data-parent="#accordion" href="#collapse1">Sliding Doors</a>
                            </h4>
                        </div>
                        <div id="collapse1" class="panel-collapse collapse">
                            <div class="panel-body">
                                <div class="tab-pane" id="1b">

                                    <div class="container-fluid">
                                        <div class="col-lg-12 table-responsive">
                                            <table st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                                                <thead class="text-center text-info">
                                                    <tr>
                                                        <th rowspan="2">NO</th>
                                                        <th colspan="2">Size</th>
                                                        <th colspan="2">Surface</th>
                                                        <th colspan="3">Location</th>
                                                        <th rowspan="2">Type</th>
                                                        <th rowspan="2">Support</th>
                                                        <th rowspan="2">Trim</th>
                                                        <th rowspan="2">Pilot</th>
                                                        <th rowspan="2">Remarks</th>
                                                    </tr>
                                                    <tr>
                                                        <th>Width (mm)</th>
                                                        <th>Height (mm)</th>
                                                        <th>Assign To</th>
                                                        <th>On Bay</th>
                                                        <th>X</th>
                                                        <th>Y</th>
                                                        <th>Of</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="x in building.Openings.SlidingDoors.SlidingDoor track by $index">
                                                        <td>
                                                            <div>{{x.ID = $index + 1}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.Width}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.Height}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.AssignTo}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.On}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.LocationX}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.LocationY}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.LocationFrom}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.Type}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.Support}}</div>
                                                        </td>
                                                        <td>
                                                            <div>{{x.Trims}}</div>
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" ng-disabled="true" ng-model="x.PilotDoor" />
                                                        </td>
                                                        <td>
                                                            <div>{{x.Remarks.Remark[0].Remark}}</div>
                                                        </td>
                                                        <td style="width:50px;">
                                                            <button type="button" ng-click="AccUpdate($index)" class="btn btn-sm">
                                                                <i class="glyphicon glyphicon-pencil">
                                                                </i>
                                                            </button>
                                                        </td>
                                                        <td>
                                                            <button type="button" ng-click="removeAcc($index)" class="btn btn-sm btn-danger">
                                                                <i class="glyphicon glyphicon-remove">
                                                                </i>
                                                            </button>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <button type="button" ng-click="ResetAcc()" class="btn btn-primary" data-toggle="modal" data-target="#AccModal">Add {{AccName}}</button>
                                                </div>
                                                <div class="col-lg-6">
                                                    <button class="btn btn-warning" ng-show="Acchistory.length > 0" ng-click="UndoAcc()">Undo Delete</button>
                                                </div>
                                            </div>
                                            <div class="modal fade" role="dialog" id="AccModal">
                                                <div class="modal-dialog modal-lg">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                            <h4 class="modal-title">{{Accword}} {{AccName}}  {{newobj.ID}}</h4>
                                                        </div>
                                                        <div class="modal-body" ng-model="newobj">
                                                            <div class="container-fluid">
                                                                <div class="col-xs-6">
                                                                    <div class="col-lg-12 well">
                                                                        <h4>Surface</h4>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Assign To Surface:
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <select class="form-control" name="AssignTo" ng-model="newobj.AssignTo" ng-options="x for x in AssignTos" ng-change="AssignVhange()"></select>
                                                                                <span style="color:red" ng-show="myForm.AssignTo.$dirty && myForm.AssignTo.$invalid">
                                                                                    <span ng-show="myForm.AssignTo.$error.required">Required.</span>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-lg-6">
                                                                                <input type="radio" checked ng-model="myVar" ng-value="true" ng-change="changeRef()">{{BayOrSpan}}
                                                                            </div>
                                                                            <div class="col-lg-6">
                                                                                <input type="radio" ng-model="myVar" ng-value="false" ng-change="changeRef()">Surface
                                                                            </div>
                                                                        </div>
                                                                        <div ng-switch="myVar">
                                                                            <div ng-switch-when="true">
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-6">
                                                                                        On {{newobj.ReferenceTo}}s :
                                                                                    </div>
                                                                                    <div class="col-xs-6" ng-if="newobj.ReferenceTo=='BAY'">
                                                                                        <select class="form-control" ng-model="newobj.On">
                                                                                            <option ng-repeat="x in building.GeneralData.Bays.Bay track by $index" value="{{x.ID}}">{{x.ID}}</option>
                                                                                        </select>
                                                                                    </div>
                                                                                    <div class="col-xs-6" ng-if="newobj.ReferenceTo=='Span'">
                                                                                        <select class="form-control" ng-model="newobj.On" ng-options="x for x in Spans"></select>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div ng-switch-when="false">

                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-xs-6">
                                                                    <div class="col-lg-12 well">

                                                                        <h4>Location</h4>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                X Coord (m):
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <input class="form-control" name="LocationX" type="number" min="0" required ng-model="newobj.LocationX" />
                                                                                <span style="color:red" ng-show="myForm.LocationX.$dirty && myForm.LocationX.$invalid">
                                                                                    <span ng-show="myForm.LocationX.$error.required">Required.</span>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Y Coord (m):
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <input class="form-control" name="LocationY" type="number" min="0" required ng-model="newobj.LocationY" />
                                                                                <span style="color:red" ng-show="myForm.LocationY.$dirty && myForm.LocationY.$invalid">
                                                                                    <span ng-show="myForm.LocationY.$error.required">Required.</span>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Of:
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <select class="form-control" ng-model="newobj.LocationFrom" ng-options="x for x in LocationFroms"></select>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-xs-6">
                                                                    <div class="col-lg-12 well">
                                                                        <h4>Size</h4>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Width (mm):
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <input class="form-control" name="Width" type="number" min="0" required ng-model="newobj.Width" />
                                                                                <span style="color:red" ng-show="myForm.Width.$dirty && myForm.Width.$invalid">
                                                                                    <span ng-show="myForm.Width.$error.required">Required.</span>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Height (mm):
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                <input class="form-control" name="Height" type="number" min="0" required ng-model="newobj.Height" />
                                                                                <span style="color:red" ng-show="myForm.Height.$dirty && myForm.Height.$invalid">
                                                                                    <span ng-show="myForm.Height.$error.required">Required.</span>
                                                                                </span>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-lg-12 Marg">
                                                                            <div class="col-xs-6">
                                                                                Area (m2):
                                                                            </div>
                                                                            <div class="col-xs-6">
                                                                                {{newobj.Height*newobj.Width}}
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-xs-6">
                                                                    <div class="col-lg-12 well">
                                                                        <div class="col-lg-12 Marg">
                                                                            <h4>Remarks</h4>
                                                                            <div class="col-xs-12">
                                                                                <textarea style="min-height:108px;max-height:108px;" class="form-control" ng-model="newobj.Remarks.Remark[0].Remark"></textarea>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-xs-12">
                                                                    <div class="col-lg-12 well">
                                                                        <h4>
                                                                            Advanced
                                                                            <span class="glyphicon glyphicon-circle-arrow-down" ng-click="showadv=true" ng-show="!showadv"></span>
                                                                            <span class="glyphicon glyphicon-circle-arrow-up" ng-click="showadv=false" ng-show="showadv"></span>
                                                                        </h4>
                                                                        <div ng-if="showadv" ng-init="newobj.Type='SINGLE LEFT';newobj.Trims='SAME AS WALL';newobj.Support='B1';
                                                                             newobj.PilotDoor=false;newobj.ExteriorPanel={ PanelType: 'NONE'}">
                                                                            <div class="col-lg-6">
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-6">
                                                                                        Type:
                                                                                    </div>
                                                                                    <div class="col-xs-6">
                                                                                        <select class="form-control" ng-model="newobj.Type" ng-options="x for x in newobjTypes"></select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-6">
                                                                                        Support:
                                                                                    </div>
                                                                                    <div class="col-xs-6">
                                                                                        <select class="form-control" ng-model="newobj.Support" ng-options="x for x in Supports"></select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-6">
                                                                                        Exterior Panel:
                                                                                    </div>
                                                                                    <div class="col-xs-6">
                                                                                        <select ng-change="initPanel()" class="form-control" ng-model="newobj.ExteriorPanel.PanelType" ng-options="x for x in PanelTypes"></select>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-6">
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-6">
                                                                                        Trims:
                                                                                    </div>
                                                                                    <div class="col-xs-6">
                                                                                        <select class="form-control" ng-model="newobj.Trims" ng-options="x for x in Trimss"></select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-lg-12 Marg">
                                                                                    <div class="col-xs-12">
                                                                                        <input type="checkbox" ng-model="newobj.PilotDoor" />  Pilot Door
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-12" ng-if="newobj.ExteriorPanel.PanelType=='ZS'">
                                                                                
                                                                                <br />

                                                                                <div class="col-lg-12 well">
                                                                                    <h5 class="text-primary"><u>Panel</u></h5>
                                                                                    <div class="row">
                                                                                        <div class="col-lg-6 Marg">Panel By</div>
                                                                                        <div class="col-lg-6 Marg">
                                                                                            <div class="col-xs-12">
                                                                                                <select class="form-control" ng-model="newobj.ExteriorPanel.PanelProfile.Panel.ByWho" ng-options="x for x in ByWho" ng-change="NSWByWhoChange()"></select>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div ng-switch="newobj.ExteriorPanel.PanelProfile.Panel.ByWho">
                                                                                        <div ng-switch-when="OTHER">
                                                                                        </div>
                                                                                        <div ng-switch-when="NONE">
                                                                                        </div>
                                                                                        <div ng-switch-when="ZS">
                                                                                            <div class="row">
                                                                                                <div class="col-lg-6 Marg">Description</div>
                                                                                                <div class="col-lg-6 Marg">
                                                                                                    <div class="col-xs-12">
                                                                                                        <h4> {{newobj.ExteriorPanel.PanelProfile.Panel.PanelProfile.Description}} </h4>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="row">
                                                                                                <div class="col-lg-2 Marg">
                                                                                                    <button data-toggle="modal" data-target="#SingleModal" class="btn btn-primary" ng-click="RestSingleWallPanelNSW()">Single Skin</button>
                                                                                                </div>
                                                                                                <div class="col-lg-2 Marg">
                                                                                                    <button data-toggle="modal" data-target="#TempconModal" class="btn btn-primary" ng-click="RestTempconWallPanelNSW()">Tempcon</button>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="col-lg-12 well">
                                                                                    <h5 class="text-primary"><u>Insulation</u></h5>
                                                                                    <div class="row">
                                                                                        <div class="col-lg-6 Marg"> Insulation By</div>
                                                                                        <div class="col-lg-6 Marg">
                                                                                            <div class="col-xs-12">
                                                                                                <select class="form-control" ng-model="newobj.ExteriorPanel.PanelProfile.Insulation.ByWho" ng-options="x for x in ByWho" ng-change="WallInsulationNSWByWho()"></select>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div ng-switch="newobj.ExteriorPanel.PanelProfile.Insulation.ByWho">
                                                                                        <div ng-switch-when="OTHER">

                                                                                        </div>
                                                                                        <div ng-switch-when="ZS">
                                                                                            <div class="row">
                                                                                                <div class="col-lg-6 Marg">Description</div>
                                                                                                <div class="col-lg-6 Marg">
                                                                                                    <div class="col-xs-12">
                                                                                                        <h4> {{newobj.ExteriorPanel.PanelProfile.Insulation.Insulation.Description}}</h4>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="row">
                                                                                                <div class="col-lg-2 Marg">
                                                                                                    <button data-toggle="modal" data-target="#InsulationModal" class="btn btn-primary" ng-click="RestWallInsulationNSW()">Insulation</button>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div ng-switch-when="NONE">
                                                                                        </div>
                                                                                    </div>
                                                                                </div>

                                                                                <div class="col-lg-12 well">
                                                                                    <h5 class="text-primary"><u>Liner</u></h5>
                                                                                    <div class="row">
                                                                                        <div class="col-lg-6 Marg"> Liner By</div>
                                                                                        <div class="col-lg-6 Marg">
                                                                                            <div class="col-xs-12">
                                                                                                <select class="form-control" ng-model="newobj.ExteriorPanel.PanelProfile.Liner.ByWho" ng-options="x for x in ByWho" ng-change="WallLinerNSWByWho()"></select>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div ng-switch="newobj.ExteriorPanel.PanelProfile.Liner.ByWho">
                                                                                        <div ng-switch-when="OTHER">
                                                                                        </div>
                                                                                        <div ng-switch-when="ZS">
                                                                                            <div class="row">
                                                                                                <div class="col-lg-6 Marg">Description</div>
                                                                                                <div class="col-lg-6 Marg">
                                                                                                    <div class="col-xs-12">
                                                                                                        <h4>{{newobj.ExteriorPanel.PanelProfile.Liner.Liner.Description}}</h4>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                            <div class="row">
                                                                                                <div class="col-lg-2 Marg">
                                                                                                    <button data-toggle="modal" data-target="#SingleModal" class="btn btn-primary" ng-click="RestSingleWallLinerNSW()">Single Skin</button>
                                                                                                </div>
                                                                                                <div class="col-lg-2 Marg">
                                                                                                    <button data-toggle="modal" data-target="#TempconModal" class="btn btn-primary" ng-click="RestTempconWallLinerNSW()">Tempcon</button>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div ng-switch-when="NONE">
                                                                                        </div>
                                                                                    </div>
                                                                                </div>


                                                                            </div>
                                                                        </div>
                                                                        <div ng-if="!showadv">
                                                                            {{newobj.Type=''}}
                                                                            {{newobj.Trims=''}}
                                                                            {{newobj.Support=''}}
                                                                            {{newobj.PilotDoor=''}}
                                                                            {{newobj.ExteriorPanel=''}}
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="modal-footer">
                                                            <div class="container-fluid">
                                                                <div class="col-lg-6">
                                                                    <button data-dismiss="modal" class="form-control" ng-click="AddUpdateAcc()"
                                                                            ng-disabled="myForm.AssignTo.$dirty && myForm.AssignTo.$invalid
                                                                                       ||myForm.LocationX.$dirty && myForm.LocationX.$invalid
                                                                                       ||myForm.LocationY.$dirty && myForm.LocationY.$invalid
                                                                                       ||myForm.Width.$dirty && myForm.Width.$invalid
                                                                                       ||myForm.Height.$dirty && myForm.Height.$invalid
                                                                                       ||myForm.LocationFrom.$dirty && myForm.LocationFrom.$invalid
                                                                                       ||myForm.CarryingWeight.$dirty && myForm.CarryingWeight.$invalid">
                                                                        Confirm
                                                                    </button>
                                                                </div>


                                                                <div class="modal fade" role="dialog" id="SingleModal">
                                                                    <div class="modal-dialog modal-md">
                                                                        <div class="modal-content">
                                                                            <div class="modal-header">
                                                                                <button type="button" class="close" ng-click="closesingle()">&times;</button>
                                                                                <h4 class="modal-title">Single Skin</h4>
                                                                            </div>
                                                                            <div class="modal-body" ng-model="newSkinobj">
                                                                                <div class="container-fluid">
                                                                                    <div class="col-lg-12 Marg">
                                                                                        <div class="col-xs-4">
                                                                                            Profile:
                                                                                        </div>
                                                                                        <div class="col-xs-8">
                                                                                            <select class="form-control" ng-model="newSkinobj.Profile" ng-change="updateDisc()" ng-options="x for x in Profile"></select>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col-lg-12 Marg">
                                                                                        <div class="col-xs-4">
