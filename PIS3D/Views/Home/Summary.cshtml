<div ng-controller="SummaryCtrl" id="myDiv" ">

    <div class="col-lg-12">
        <div class="well" style="font-size:12px;">
            <h2 style="text-align:center;">Project Information System </h2>
            <div class="container-fluid" style="border:solid;border-width: 1px;border-radius:5px;">
                <div class="col-lg-12 Marg">
                    <table>
                        <tr>
                            <th style="width:30%">
                                <div class="col-lg-12">
                                    <div class="col-lg-12"> Job / Quotation No : {{Job}}</div>
                                    <div class="col-lg-12">Building No : {{building.GeneralData.ID}}</div>
                                    <div class="col-lg-12">Area No : </div>
                                    <div class="col-lg-12">Revision Date : </div>
                                </div>
                            </th>
                            <th style="width:30%">
                                <div class="col-lg-12">
                                    <img src="/Image/ZamiLogo.png" style="margin-left:auto;margin-right:auto;display:block;width:130px;" />
                                </div>
                            </th>
                            <th>
                                <div class="col-lg-11 col-lg-offset-1">
                                    <div class="col-lg-12">QIF No : </div>
                                    <div class="col-lg-12">QIF Revision : </div>
                                    <div class="col-lg-12">QIF Revision Date : </div>
                                    <div class="col-lg-12">Customer : </div>
                                </div>
                            </th>
                        </tr>
                    </table>

                </div>
            </div>

            <div class="container-fluid">
                <div class="row">
                    <h5 class="text-primary">General Data</h5>
                    <div class="col-lg-12">
                        <table st-table="rowCollection" class="table  table-condensed table-striped">
                            <tr>
                                <th> Job / Quotation No </th>
                                <th>{{Job}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Building Number</th>
                                <th>{{building.GeneralData.ID}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Width</th>
                                <th>{{building.GeneralData.Width}}</th>
                                <th>Width Refrance</th>
                                <th>{{building.GeneralData.WidthReference}}</th>
                            </tr>
                            <tr>
                                <th>Lentgh</th>
                                <th>{{building.GeneralData.Length}}</th>
                                <th>Length Reference</th>
                                <th>{{building.GeneralData.LengthReference}}</th>
                            </tr>
                            <tr>
                                <th> Height Reference</th>
                                <th>{{building.GeneralData.HeightReference}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th> Frame Type</th>
                                <th>{{building.GeneralData.FrameType}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Fixed Base Allowed</th>
                                <th>{{building.GeneralData.FixedBaseAllowed}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th> Frame Type</th>
                                <th>{{building.GeneralData.FrameType}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Bays Spacing  </th>
                                <th>{{GetBaysProfile()}}</th>
                                <th></th>
                                <th></th>
                            </tr>

                        </table>
                    </div>
                    <div class="col-lg-12">
                        <table st-table="rowCollection" class="table  table-condensed table-striped">
                            <tr>
                                <th><h6 class="text-primary">Future Extenssions</h6></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th> Longitudinal Direction</th>
                                <th>{{building.GeneralData.FutureExtension.LongitudinalDirection}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th> Transverse Direction</th>
                                <th>{{building.GeneralData.FutureExtension.TransverseDirection}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                        </table>
                    </div>
                    <div class="col-lg-12">
                        <h6 class="text-primary">Building Location</h6>
                        <table st-table="rowCollection" class="table  table-condensed table-striped">
                            <tr>
                                <th>Along Building</th>
                                <th>From Gl</th>
                                <th>To Gl</th>
                                <th>Axes</th>
                            </tr>
                            <tr>
                                <th>Width GL </th>
                                <th>{{building.GeneralData.Location.AlongBuildingWidth.FromGL}}</th>
                                <th>{{building.GeneralData.Location.AlongBuildingWidth.ToGL}}</th>
                                <th>{{building.GeneralData.Location.AlongBuildingWidth.Axes}}</th>
                            </tr>
                            <tr>
                                <th>Length GL </th>
                                <th>{{building.GeneralData.Location.AlongBuildingLength.FromGL}}</th>
                                <th>{{building.GeneralData.Location.AlongBuildingLength.ToGL}}</th>
                                <th>{{building.GeneralData.Location.AlongBuildingLength.Axes}}</th>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <hr />
            <h5 class="text-primary">Geometry </h5>
            <div class="container-fluid">
                <table class="text-center">
                    <tr>
                        <th>
                            @*Bay Table*@
                            <div class="col-lg-12">
                                <h5 class="text-info">BAYS</h5>
                                <table st-table="rowCollection" class="table  table-condensed table-striped table-sm  table-bordered text-center">
                                    <thead class="text-center text-info">
                                        <tr>
                                            <th>ID</th>
                                            <th>Length (m)</th>
                                            <th>Bracing Available</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr id="BayRepeater" ng-repeat="x in building.GeneralData.Bays.Bay track by $index">
                                            <td>
                                                <div>{{x.ID}}</div>
                                            </td>
                                            <td>
                                                {{ x.Width }}
                                            </td>
                                            <td>
                                                <div><input ng-disabled="true" type="checkbox" ng-model="x.BracingAvailable" /></div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                                @*Bay Spacing and Lengh*@
                                @*<div class="row">
                                        <div class="container-fluid">
                                            <br />
                                            <h6><strong> Bays Spacing :  {{GetBaysProfile()}} </strong></h6>
                                        </div>
                                    </div>*@
                                @*End*@
                            </div>
                            @*End Bay*@
                        </th>
                        <th>
                            @*Frame*@
                            <div class="col-lg-12">
                                <h5 class="text-info">Frames</h5>
                                <table st-table="rowCollection" class="table  table-condensed table-striped table-sm  table-bordered text-center">
                                    <thead class="text-center text-info">
                                        <tr>
                                            <th>Frame ID</th>
                                            <th>Profile ID</th>
                                        </tr>
                                    </thead>
                                    <tbody id="FrameRepeater">
                                        <tr ng-repeat="x in building.GeneralData.Frames.FrameProfile  track by $index">
                                            <td>
                                                <div>{{x.ID}}</div>
                                            </td>
                                            <td>
                                                <div>{{x.ProfileNo}}</div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            @*End Frame*@
                        </th>
                        <th>
                            @*Profiles*@
                            <div class="col-lg-12">
                                <h5 class="text-info">Profiles</h5>
                                <div class="container-fluid">
                                    <div class="row" ng-repeat="p in building.GeneralData.Profiles.Profile track by p.ID">
                                        <div class="container-fluid">
                                            <div class="col-lg-12">
                                                <div class="col-lg-12">
                                                    <h5 class="text-info">Profile No {{p.ID}} </h5>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12" ng-repeat="g in p.GablesWidthModules.GableWidthModules track by g.GableNo">
                                                        <div class="container-fluid" style="margin-bottom: 7px;">
                                                            <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                                                Gable # : {{gn}}
                                                            </div>
                                                            <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                                                <div class="row">
                                                                    <div class="col-lg-8" style="word-wrap: break-word;">
                                                                        Width Module : {{wg.Width.toString()}}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            @*End Profiles*@
                        </th>
                    </tr>
                </table>
            </div>

            @*Gables*@
            <div class="container-fluid">
                <div class="row">
                    <h5 class="text-info col-lg-12">Gables</h5>
                    <div class="col-lg-12   ">
                        <table st-table="rowCollection" class="table  table-condensed table-sm  table-bordered text-center">
                            <thead class="text-center text-info">
                                <tr>
                                    <th>ID</th>
                                    <th>Width (m)</th>
                                    <th>Ridge Distance (m)</th>
                                    <th>Left Col Base (m)</th>
                                    <th>Left Height (m)</th>
                                    <th>Left Slope (:10)</th>
                                    <th>Right Col Base (m)</th>
                                    <th>Right Height (m)</th>
                                    <th>Right Slope (:10)</th>
                                    <th>NS Eave Cond</th>
                                    <th>NS Downspouts</th>
                                    <th>Fs Eave Cond</th>
                                    <th>FS Downspouts</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="GableRepeater" ng-repeat="x in building.GeneralData.Gables.Gable track by $index">
                                    <td>
                                        <div>{{x.ID}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.Width}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RidgeDistance}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftColBase}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftHeight}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftSlope}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightColBase}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightHeight}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightSlope}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.NearSide.EaveConditionEnum}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.NearSide.Downspouts}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.FarSide.EaveConditionEnum}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.FarSide.Downspouts}}</div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="col-lg-12">
                    </div>
                </div>
                <br />
            </div>
            @*End Gables*@
            <div class="container-fluid">
                <h6 class="text-primary"> End Walls </h6>
                <table st-table="rowCollection" class="table  table-condensed table-striped">
                    <tr>
                        <th>Wall</th>
                        <th>Type</th>
                        <th>Wind Columns (m)</th>
                    </tr>
                    <tr>
                        <th>LEW</th>
                        <th>{{building.GeneralData.EndWalls.LEW.Type}}</th>
                        <th>
                            <div class="container-fluid" ng-repeat="g in building.GeneralData.EndWalls.LEW.Profile.GablesWidthModules.GableWidthModules track by g.GableNo" style="margin-top:10px;">
                                <div class="row">
                                    <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                        <strong> Gable # : </strong> {{gn}}
                                    </div>
                                    <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                        <div class="row">
                                            <div class="col-lg-8" style="word-wrap: break-word;">
                                                <strong> Width Module : </strong> {{wg.Width.toString()}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </th>
                    </tr>
                    <tr>
                        <th>REW</th>
                        <th>{{building.GeneralData.EndWalls.REW.Type}}</th>
                        <th>
                            <div class="container-fluid" ng-repeat="g in building.GeneralData.EndWalls.REW.Profile.GablesWidthModules.GableWidthModules track by g.GableNo" style="margin-top:10px;">
                                <div class="row">
                                    <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                        <strong> Gable # : </strong> {{gn}}
                                    </div>
                                    <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                        <div class="row">
                                            <div class="col-lg-8" style="word-wrap: break-word;">
                                                <strong> Width Module : </strong> {{wg.Width.toString()}}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </th>
                    </tr>
                </table>
            </div>
            @*General Remarks*@
            <div class="container-fluid">
                <table ng-show="building.GeneralData.Remarks.Remark" st-table="rowCollection" class="table  table-condensed table-striped">
                    <thead class="text-left text-info">
                        <tr>
                            @*<th>NO</th>*@
                            <th>Remarks</th>
                        </tr>
                    </thead>
                    <tbody style="text-align: left;">
                        <tr ng-repeat="x in building.GeneralData.Remarks.Remark track by $index">
                            @*<td style="width:50px;" ng-hide="(x.State == 'false')">
                                    <div>{{x.ID = $index + 1}}</div>
                                </td>*@
                            <td ng-hide="(x.State == 'false')">
                                <div ng-class="{'text-warning':(x.State == 'false') }" style="white-space:pre-line"><div ng-bind-html="x.Remark"></div></div>
                            </td>
                        </tr>
                    </tbody>
                </table>

            </div>

            @*Load &codes*@
            <div class="container-fluid" style="font-size:12px;margin-left: -14px;">
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Design Codes</u></h5>
                    <div class="col-lg-12">
                        <table st-table="rowCollection" class="table  table-condensed table-striped">
                            <tr>
                                <th>Dead Load (KN/m2)</th>
                                <th>
                                    <div class="col-lg-12">
                                        User Value <input type="checkbox" ng-disabled="true" name="AsPerDesign" ng-model="AsPerDesign" required>
                                        <div ng-if="!AsPerDesign">
                                            <span ng-show="!AsPerDesign" class="text-warning">As Per Design</span>
                                            {{building.DesignCode.DeadLoad.UserValue = ""}}
                                        </div>
                                        <div ng-if="AsPerDesign">{{building.DesignCode.DeadLoad.UserValue}}</div>
                                    </div>
                                </th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Live Load on Roof (KN/m2)</th>
                                <th>{{building.DesignCode.LiveLoadOnRoof}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Live Load on Frame (KN/m2)</th>
                                <th>{{building.DesignCode.LiveLoadOnFrame}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Collateral Load (KN/m2)</th>
                                <th>{{building.DesignCode.CollateralLoad}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr ng-switch="myVar">
                                {{myVarfun()}}
                                <th ng-switch-when="true">
                                    Wind Speed (Kph)
                                </th>
                                <th ng-switch-when="true">
                                    {{building.DesignCode.Wind.WindSpeed}}
                                </th>
                                <th ng-switch-when="true">
                                    {{building.DesignCode.Wind.WindSpeedEnum}}
                                </th>
                                <th ng-switch-when="false">
                                    Wind Pressure (KN/m2)
                                </th>
                                <th ng-switch-when="false">
                                    {{building.DesignCode.Wind.WindPressure}}
                                </th>
                                <th ng-switch-when="false"></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Exposure Category</th>
                                <th>{{building.DesignCode.Wind.WindExposure}}</th>
                                <th></th>
                                <th></th>
                            </tr>
                            <tr>
                                <th>Design Codes</th>
                                <th>{{building.DesignCode.DesignCodes}}</th>
                                <th>Apply Loads Per</th>
                                <th>{{building.DesignCode.ApplyLoadsPer}}</th>
                            </tr>
                            <tr>
                                <th>Cold Formed Design Codes</th>
                                <th>{{building.DesignCode.ColdFormedDesignCodes}}</th>
                                <th>Seismic</th>
                                <th>{{building.DesignCode.Seismic}}</th>
                            </tr>
                            <tr>
                                <th>Ground Snow (KN/m2)</th>
                                <th>{{building.DesignCode.GroundSnowLoad}}</th>
                                <th>Thickness Ground Snow (mm)</th>
                                <th>{{building.DesignCode.ThicknessGroundSnow}}</th>
                            </tr>
                            <tr>
                                <th>Heavy Rain Area</th>
                                <th>{{building.DesignCode.RainFall.HeavyRainArea}}</th>
                                <th>Rain Fall (mm/h)</th>
                                <th>{{building.DesignCode.RainFall.Intensity}}</th>
                            </tr>
                            <tr>
                                <th>Return Period (Years)</th>
                                <th>{{building.DesignCode.RainFall.Years}}</th>
                                <th>Duration (Minutes)</th>
                                <th>{{building.DesignCode.RainFall.Minutes}}</th>
                            </tr>
                            <tr>
                                <th>Temperature (Deg) Minus</th>
                                <th>{{building.DesignCode.Temperature.Minus}}</th>
                                <th>Temperature (Deg) Plus</th>
                                <th>{{building.DesignCode.Temperature.Plus}}</th>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            @*End Load &codes*@

            @*Load &codes Remarks*@
            <div class="container-fluid">
                <table ng-show="building.DesignCode.Remarks.Remark" st-table="rowCollection" class="table  table-condensed table-striped">
                    <thead class="text-left text-info">
                        <tr>
                            @*<th>NO</th>*@
                            <th>Remarks</th>
                        </tr>
                    </thead>
                    <tbody style="text-align: left;">
                        <tr ng-repeat="x in building.DesignCode.Remarks.Remark track by $index">
                            @*<td style="width:50px;" ng-hide="(x.State == 'false')">
                                    <div>{{x.ID = $index + 1}}</div>
                                </td>*@
                            <td ng-hide="(x.State == 'false')">
                                <div ng-class="{'text-warning':(x.State == 'false') }" style="white-space:pre-line"><div ng-bind-html="x.Remark"></div></div>
                            </td>
                        </tr>
                    </tbody>
                </table>

            </div>

            @*Surface Treatment*@
            <div class="container-fluid" style="font-size:12px;margin-left: -14px;">
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Primary Members</u></h5>
                    <table st-table="rowCollection" class="table  table-condensed table-striped">
                        <tr>
                            <th>Primary Members</th>
                            <th>{{building.SurfaceTreatment.PrimaryMembers.SurfaceFinish}}</th>
                            <th></th>
                            <th></th>
                        </tr>
                        <tr>
                            <th>Primary Surface Treatment</th>
                            <th>{{building.SurfaceTreatment.PrimaryMembers.SurfaceTreatment}}</th>
                            <th></th>
                            <th></th>
                        </tr>
                        <tr>
                            <th>Primary Paint System</th>
                            <th>{{building.SurfaceTreatment.PrimaryMembers.PaintSystem}}</th>
                            <th></th>
                            <th></th>
                        </tr>
                    </table>
                </div>
                <div class="col-lg-12 Marg">
                    <table ng-show="building.SurfaceTreatment.PrimaryMembers.PaintSystem == 'SPECIAL' && building.SurfaceTreatment.PrimaryMembers.SurfaceFinish == 'PAINTED'" st-table="rowCollection" class="table  table-condensed table-striped table-sm  table-bordered text-center">
                        <thead class="text-left text-info">
                            <tr>
                                <th>NO</th>
                                <th>BY</th>
                                <th>Thickness</th>
                                <th>Manufacturer</th>
                                <th>Paint Code</th>
                                <th>Color</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="x in building.SurfaceTreatment.PrimaryMembers.Coats.Coat track by $index">
                                <td>
                                    <div>{{x.ID = $index + 1}}</div>
                                </td>
                                <td>
                                    <div>{{x.By_Who}}</div>
                                </td>
                                <td>
                                    <div>{{x.Thickness}}</div>
                                </td>
                                <td>
                                    <div>{{x.Manufacturer}}</div>
                                </td>
                                <td>
                                    <div>{{x.PaintCode}}</div>
                                </td>
                                <td>
                                    <div>{{x.Color}}</div>
                                </td>
                                <td>
                                    <div>{{x.Description}}</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                @*PrimaryRemarks*@
                <div class="container-fluid">
                    <table ng-show="building.SurfaceTreatment.PrimaryMembers.Remarks.Remark" st-table="rowCollection" class="table  table-condensed table-striped">
                        <thead class="text-left text-info">
                            <tr>
                                @*<th>NO</th>*@
                                <th>Remarks</th>
                            </tr>
                        </thead>
                        <tbody style="text-align: left;">
                            <tr ng-repeat="x in building.SurfaceTreatment.PrimaryMembers.Remarks.Remark track by $index">
                                @*<td style="width:50px;" ng-hide="(x.State == 'false')">
                                        <div>{{x.ID = $index + 1}}</div>
