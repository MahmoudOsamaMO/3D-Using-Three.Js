
@{
    ViewBag.Title = "Summary";
}
<style>
    tree {
        margin-left: 20px;
        display: block;
    }
</style>
<script src="~/Scripts/jquery.dataTables.js"></script>

   

@*<h2>Summary</h2>
    <div ng-controller="SummaryCtrl">
        <tree family="treeFamily">
        </tree>
        {{building | json}}
    </div>*@

<script src="~/Scripts/Site/Summary.js"></script>


<link href="~/Content/Cifp.css" rel="stylesheet" />
<style type="text/css">
    .auto-style1 {
        border-bottom: 1px solid #cccccc;
        font-size: 10px;
        border-left-width: 1;
        border-right-width: 1;
        border-top-width: 1;
        width: 22%;
    }

    .auto-style3 {
        border-bottom: 1px solid #cccccc;
        font-size: 10px;
        border-left-width: 1;
        border-right-width: 1;
        border-top-width: 1;
        width: 19%;
    }

    .auto-style4 {
        border-bottom: 1px solid #CCCCCC;
        font-size: 10px;
        text-transform: uppercase;
        border-left-width: 1;
        border-right-width: 1;
        border-top-width: 1;
        padding-left: 4;
        padding-top: 4;
        width: 39%;
    }

    .auto-style5 {
        border-bottom: 1px solid #cccccc;
        font-size: 10px;
        border-left-width: 1;
        border-right-width: 1;
        border-top-width: 1;
        width: 30%;
    }

    .auto-style8 {
        border-bottom: 1px solid #FFFFFF;
        font-size: 10px;
        border-left-width: 1;
        border-right-width: 1;
        border-top-width: 1;
        height: 17px;
    }
</style>
<div ng-controller="SummaryCtrl" id="myDiv">

    <div class="col-lg-12">
        <div class="well" style="font-size:12px;">
            <div class="container-fluid">
                <div class="row">
                    <h6 class="text-primary">Information</h6>
                    <div class="col-lg-6">
                        ID : {{building.GeneralData.ID}}<br />
                        Width Refrance : {{building.GeneralData.WidthReference}}<br />
                        Length Reference : {{building.GeneralData.LengthReference}}<br />
                        Height Reference : {{building.GeneralData.HeightReference}}<br />
                        <h6> <strong>Lentgh = {{building.GeneralData.Length}}</strong></h6>
                        <h6> <strong> Width  = {{building.GeneralData.Width}}</strong></h6>
                    </div>
                    <div class="col-lg-6">
                        Frame Type : {{building.GeneralData.FrameType}}<br />
                        Fixed Base Allowed : {{building.GeneralData.FixedBaseAllowed}}<br />
                        <h6 class="text-primary">Future Extenssions</h6>
                        Longitudinal Direction : {{building.GeneralData.FutureExtension.LongitudinalDirection}}<br />
                        Transverse Direction : {{building.GeneralData.FutureExtension.TransverseDirection}}<br />
                    </div>
                    <div class="col-lg-12">
                        <h6 class="text-primary">Location</h6>
                        <div class="col-lg-6">
                            <h6 class="text-primary">Along Building Width</h6>
                            <div class="col-lg-6">
                                From Gl : {{building.GeneralData.Location.AlongBuildingWidth.FromGL}}
                            </div>
                            <div class="col-lg-6">
                                To Gl : {{building.GeneralData.Location.AlongBuildingWidth.ToGL}}
                            </div>
                            <div class="col-lg-12">
                                Axes = {{building.GeneralData.Location.AlongBuildingWidth.Axes}}
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <h6 class="text-primary">Along Building Length</h6>
                            <div class="col-lg-6">
                                From Gl : {{building.GeneralData.Location.AlongBuildingLength.FromGL}}
                            </div>
                            <div class="col-lg-6">
                                To Gl : {{building.GeneralData.Location.AlongBuildingLength.ToGL}}
                            </div>
                            <div class="col-lg-12">
                                Axes = {{building.GeneralData.Location.AlongBuildingLength.Axes}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <hr />
            <h6 class="text-primary">Geometry </h6>
            @*Bays*@

            @*Bay Table*@
            <div class="col-lg-3">
                <h5 class="text-info">BAYS</h5>
                <table st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                    <thead class="text-center text-info">
                        <tr>
                            <th>ID</th>
                            <th>Length (m)</th>
                            <th>Bracing Available</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr id="BayRepeater" ng-repeat="x in building.GeneralData.Bays.Bay track by $index">
                            <td>
                                <div>{{x.ID}}</div>
                            </td>
                            <td>
                                {{ x.Width }}
                            </td>
                            <td>
                                <div><input ng-disabled="true" type="checkbox" ng-model="x.BracingAvailable" /></div>
                            </td>
                        </tr>
                    </tbody>
                </table>
                @*Bay Spacing and Lengh*@
                <div class="row">
                    <div class="container-fluid">
                        <br />
                        <h6><strong> Bays Spacing :  {{GetBaysProfile()}} </strong></h6>
                    </div>
                </div>
                @*End*@
            </div>
            @*End Bay*@

            @*Frame*@
            <div class="col-lg-3 table-responsive">
                <h5 class="text-info">Frames</h5>
                <table st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                    <thead class="text-center text-info">
                        <tr>
                            <th>Frame ID</th>
                            <th>Profile ID</th>
                        </tr>
                    </thead>
                    <tbody id="FrameRepeater">
                        <tr ng-repeat="x in building.GeneralData.Frames.FrameProfile  track by $index">
                            <td>
                                <div>{{x.ID}}</div>
                            </td>
                            <td>
                                <div>{{x.ProfileNo}}</div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            @*End Frame*@

            @*Profiles*@
            <div class="col-lg-6">
                <h5 class="text-info">Profiles</h5>
                <div class="container">
                    <div class="row" ng-repeat="p in building.GeneralData.Profiles.Profile track by p.ID">
                        <div class="container-fluid">
                            <div class="col-lg-6">
                                <div class="col-lg-12">
                                    <h5 class="text-info">Profile No {{p.ID}} </h5>
                                </div>
                                <div class="row">
                                    <div class="col-lg-12" ng-repeat="g in p.GablesWidthModules.GableWidthModules track by g.GableNo">
                                        <div class="container-fluid" style="margin-bottom: 7px;">
                                            <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                                Gable # : {{gn}}
                                            </div>
                                            <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                                <div class="row">
                                                    <div class="col-lg-8" style="word-wrap: break-word;">
                                                        Width Module : {{wg.Width.toString()}}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            @*End Profiles*@

            @*End Bays*@

            @*Gables*@
            <div class="container-fluid">
                <div class="row">
                    <h5 class="text-info col-lg-12">Gables</h5>
                    <div class="col-lg-12 table-responsive">
                        <table st-table="rowCollection" class="table table-sm table-responsive table-bordered text-center">
                            <thead class="text-center text-info">
                                <tr>
                                    <th>ID</th>
                                    <th>Width (m)</th>
                                    <th>Ridge Distance (m)</th>
                                    <th>Left Col Base (m)</th>
                                    <th>Left Height (m)</th>
                                    <th>Left Slope (:10)</th>
                                    <th>Right Col Base (m)</th>
                                    <th>Right Height (m)</th>
                                    <th>Right Slope (:10)</th>
                                    <th>NS Eave Cond</th>
                                    <th>NS Downspouts</th>
                                    <th>Fs Eave Cond</th>
                                    <th>FS Downspouts</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="GableRepeater" ng-repeat="x in building.GeneralData.Gables.Gable track by $index">
                                    <td>
                                        <div>{{x.ID}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.Width}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RidgeDistance}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftColBase}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftHeight}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.LeftSlope}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightColBase}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightHeight}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.RightSlope}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.NearSide.EaveConditionEnum}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.NearSide.Downspouts}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.FarSide.EaveConditionEnum}}</div>
                                    </td>
                                    <td>
                                        <div>{{x.EaveCondition.FarSide.Downspouts}}</div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="col-lg-12">
                    </div>
                </div>
                <br />
            </div>
            @*End Gables*@
            <div class="container-fluid">
                <h6 class="text-primary"> End Walls </h6>
                <div class="col-lg-6">
                    LEW<br />
                    Type:{{building.GeneralData.EndWalls.LEW.Type}}
                    <div class="container-fluid" ng-repeat="g in building.GeneralData.EndWalls.LEW.Profile.GablesWidthModules.GableWidthModules track by g.GableNo" style="margin-top:10px;">
                        <div class="row">
                            <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                <strong> Gable # : </strong> {{gn}}
                            </div>
                            <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                <div class="row">
                                    <div class="col-lg-8" style="word-wrap: break-word;">
                                        <strong> Width Module : </strong> {{wg.Width.toString()}}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    REW<br />
                    Type:{{building.GeneralData.EndWalls.REW.Type}}
                    <div class="container-fluid" ng-repeat="g in building.GeneralData.EndWalls.REW.Profile.GablesWidthModules.GableWidthModules track by g.GableNo" style="margin-top:10px;">
                        <div class="row">
                            <div class="col-lg-3" ng-repeat="gn in g.GableNo">
                                <strong> Gable # : </strong> {{gn}}
                            </div>
                            <div class="col-lg-9" ng-repeat="wg in g.WidthModules track by wg.Width">
                                <div class="row">
                                    <div class="col-lg-8" style="word-wrap: break-word;">
                                        <strong> Width Module : </strong> {{wg.Width.toString()}}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <hr />
            @*Load &codes*@
            <div class="container-fluid" style="font-size:12px;margin-left: -14px;">
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Load</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Dead Load (KN/m2)</div>
                        <div class="col-lg-6">
                            User Value <input type="checkbox" ng-disabled="true" name="AsPerDesign" ng-model="AsPerDesign" required>
                            <div ng-if="!AsPerDesign">
                                <span ng-show="!AsPerDesign" class="text-warning">As Per Design</span>
                                {{building.DesignCode.DeadLoad.UserValue = ""}}
                            </div>
                            <div ng-if="AsPerDesign">{{building.DesignCode.DeadLoad.UserValue}}</div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Live Load on Roof (KN/m2)</div>
                        <div class="col-lg-6">{{building.DesignCode.LiveLoadOnRoof}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Live Load on Frame (KN/m2)</div>
                        <div class="col-lg-6">{{building.DesignCode.LiveLoadOnFrame}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Collateral Load (KN/m2)</div>
                        <div class="col-lg-6">{{building.DesignCode.CollateralLoad}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Wind</u></h5>
                    <div ng-switch="myVar">
                        <div ng-switch-when="true">
                            <div class="col-lg-6">
                                <div class="col-lg-4">Wind Speed (Kph)</div>
                                <div class="col-lg-2">{{building.DesignCode.Wind.WindSpeed}}</div>
                                <div class="col-lg-4">{{building.DesignCode.Wind.WindSpeedEnum}}</div>
                            </div>
                        </div>
                        <div ng-switch-when="false">
                            <div class="col-lg-6">
                                <div class="col-lg-6">Wind Pressure (KN/m2)</div>
                                <div class="col-lg-6">{{building.DesignCode.Wind.WindPressure}}</div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Exposure Category</div>
                        <div class="col-lg-6">{{building.DesignCode.Wind.WindExposure}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Design Codes</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Design Codes</div>
                        <div class="col-lg-6">{{building.DesignCode.DesignCodes}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Apply Loads Per</div>
                        <div class="col-lg-6">{{building.DesignCode.ApplyLoadsPer}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Cold Formed Design Codes</div>
                        <div class="col-lg-6">{{building.DesignCode.ColdFormedDesignCodes}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Seismic</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Seismic</div>
                        <div class="col-lg-6">{{building.DesignCode.Seismic}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Snow</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Ground Snow (KN/m2)</div>
                        <div class="col-lg-6">
                            {{building.DesignCode.GroundSnowLoad}}
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Thickness Ground Snow (mm)</div>
                        <div class="col-lg-6">{{building.DesignCode.ThicknessGroundSnow}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Rain</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">HeavyRainArea</div>
                        <div class="col-lg-6">{{building.DesignCode.RainFall.HeavyRainArea}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Rain Fall (mm/h)</div>
                        <div class="col-lg-6">{{building.DesignCode.RainFall.Intensity}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Return Period (Years)</div>
                        <div class="col-lg-6">{{building.DesignCode.RainFall.Years}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Duration (Minutes)</div>
                        <div class="col-lg-6">{{building.DesignCode.RainFall.Minutes}}</div>
                    </div>
                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Temperature (Deg)</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Minus</div>
                        <div class="col-lg-6">{{building.DesignCode.Temperature.Minus}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Plus</div>
                        <div class="col-lg-6">{{building.DesignCode.Temperature.Plus}}</div>
                    </div>
                </div>
            </div>
            @*End Load &codes*@
            <hr />
            @*Surface Treatment*@
            <div class="container-fluid" style="font-size:12px;margin-left: -14px;">
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Primary Members</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Primary Members</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.PrimaryMembers.SurfaceFinish}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Primary Surface Treatment</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.PrimaryMembers.SurfaceTreatment}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Primary Paint System</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.PrimaryMembers.PaintSystem}}</div>
                    </div>
                </div>
                <div class="col-lg-12  table-responsive">
                    <table style="font-size:10px;" ng-show="building.SurfaceTreatment.PrimaryMembers.PaintSystem == 'SPECIAL' && building.SurfaceTreatment.PrimaryMembers.SurfaceFinish == 'PAINTED'" st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                        <thead class="text-left text-info">
                            <tr>
                                <th>NO</th>
                                <th>BY</th>
                                <th>Thickness</th>
                                <th>Manufacturer</th>
                                <th>Paint Code</th>
                                <th>Color</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="x in building.SurfaceTreatment.PrimaryMembers.Coats.Coat track by $index">
                                <td>
                                    <div>{{x.ID = $index + 1}}</div>
                                </td>
                                <td>
                                    <div>{{x.By_Who}}</div>
                                </td>
                                <td>
                                    <div>{{x.Thickness}}</div>
                                </td>
                                <td>
                                    <div>{{x.Manufacturer}}</div>
                                </td>
                                <td>
                                    <div>{{x.PaintCode}}</div>
                                </td>
                                <td>
                                    <div>{{x.Color}}</div>
                                </td>
                                <td>
                                    <div>{{x.Description}}</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Secondery Members</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Secondery Members</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.SecondaryMembers.SurfaceFinish}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Secondery Surface Treatment</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.SecondaryMembers.SurfaceTreatment}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">Secondery Paint System</div>
                        <div class="col-lg-6">{{building.SurfaceTreatment.SecondaryMembers.PaintSystem}}</div>
                    </div>
                </div>
                <div class="col-lg-12  table-responsive">
                    <table style="font-size:10px;" ng-show="building.SurfaceTreatment.SecondaryMembers.PaintSystem == 'SPECIAL' && building.SurfaceTreatment.PrimaryMembers.SurfaceFinish == 'PAINTED'" st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                        <thead class="text-left text-info">
                            <tr>
                                <th>NO</th>
                                <th>BY</th>
                                <th>Thickness</th>
                                <th>Manufacturer</th>
                                <th>Paint Code</th>
                                <th>Color</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="x in building.SurfaceTreatment.SecondaryMembers.Coats.Coat track by $index">
                                <td>
                                    <div>{{x.ID = $index + 1}}</div>
                                </td>
                                <td>
                                    <div>{{x.By_Who}}</div>
                                </td>
                                <td>
                                    <div>{{x.Thickness}}</div>
                                </td>
                                <td>
                                    <div>{{x.Manufacturer}}</div>
                                </td>
                                <td>
                                    <div>{{x.PaintCode}}</div>
                                </td>
                                <td>
                                    <div>{{x.Color}}</div>
                                </td>
                                <td>
                                    <div>{{x.Description}}</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
            </div>
            @*End Surface Treatment*@
            <hr />
            @*Bracing*@
            <div class="container-fluid" style="font-size:12px;margin-left: -14px;">
                <div class="col-lg-12">
                    <h5 class="text-primary"><u>Bracings</u></h5>
                    <div class="col-lg-6">
                        <div class="col-lg-6">FSW Bracing Type</div>
                        <div class="col-lg-6">{{building.Bracings.FSW}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">NSW Bracing Type</div>
                        <div class="col-lg-6">{{building.Bracings.NSW}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">LEW Bracing Type</div>
                        <div class="col-lg-6">{{building.Bracings.LEW}}</div>
                    </div>
                    <div class="col-lg-6">
                        <div class="col-lg-6">REW Bracing Type</div>
                        <div class="col-lg-6">{{building.Bracings.REW}}</div>
                    </div>
                </div>
                <div ng-show="building.Bracings.InteriorBracings.Bracing" class="col-lg-12 Marg table-responsive">
                    <table style="font-size:10px;" st-table="rowCollection" class="table table-striped table-sm table-responsive table-bordered text-center">
                        <thead class="text-center text-info">
                            <tr>
                                <th rowspan="2">ID</th>
                                <th rowspan="2">Type</th>
                                <th rowspan="2">Height</th>
                                <th colspan="2">Location At Offset</th>
                                <th colspan="1">Location At GL</th>
                            </tr>
                            <tr>
                                <th>Offset</th>
                                <th>From</th>
                                <th>AtGL</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr id="BracingsRepeater" ng-repeat="x in building.Bracings.InteriorBracings.Bracing track by $index">
                                <td>
                                    <div>{{$index + 1}}</div>
                                </td>
                                <td>
                                    <div ng-hide="editingData[x.ID]">{{x.Type}}</div>
                                </td>
                                <td>
                                    <div ng-hide="editingData[x.ID]">{{x.PortalPartialHeight}}</div>
                                </td>
                                <td>
                                    <div ng-hide="editingData[x.ID]">{{x.Location.Item.Offset}}</div>
                                </td>
                                <td>
                                    <div ng-hide="editingData[x.ID]">{{x.Location.Item.From}}</div>
                                </td>
                                <td>
                                    <div ng-if="isNumber(x.Location.Item)" ng-hide="editingData[x.ID]">{{x.Location.Item}}</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                </div>
            </div>
            @*END Bracing*@
